            grid-template-rows: repeat(4, 1fr);
        }

        .mosaic.length-5 {
            grid-template-rows: repeat(7, 1fr);
        }

        .mosaic.length-5 .item-2 {
            grid-column: 3 / span 4;
            grid-row: 1 / span 4;
        }

        .mosaic.length-5 .item-4 {
            grid-row: 5 / span 3;
            grid-column: 1 / span 3;
        }

        .mosaic.length-5 .item-5 {
            grid-row: 5 / span 3;
            grid-column: 4 / span 3;
        }

        .mosaic.length-6 {
            grid-template-rows: repeat(6, 1fr);
        }

        .mosaic.length-6 .item-2 {
            grid-column: 3 / span 4;
            grid-row: 1 / span 4;
        }
    }
</style>



