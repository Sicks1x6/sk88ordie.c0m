<script>
  function openProductOrderForm(button, event, productData) {
    if(event.defaultPrevented) {
      return;
    }
    event.preventDefault();
    const position = button.getAttribute('data-position');
    const section = 'Product';

    const {
      productId,
      productItemImgUrl,
      productItemHeading,
      productItemPrice,
      currencySymbol,
      visibility,
    } = productData;

    // Create the modal content dynamically
    const modalContent = `
            <div class="order-form-wrapper" id="product-modal">
                <div class="order-content-default">
                    ${visibility.image && productItemImgUrl ? `
                        <div class="order-thumbnail">
                            <img src="${productItemImgUrl}" alt="Product Image" loading="lazy" />
                        </div>
                    ` : ''}

                    <div class="order-details">
                        ${visibility.heading && productItemHeading ? `
                            <div class="card-title heading-3 cs-mb-8">${productItemHeading}</div>
                        ` : ''}

                        <div class="card-text body-text-2">
                            ${visibility.price && productItemPrice ? `
                                <div class="product-price brand-color text-start">
                                    <span class="heading-5">${currencySymbol}</span>
                                    <span class="heading-5">${productItemPrice}</span>
                                </div>
                            ` : ''}
                        </div>

                        ${(visibility.heading && productItemHeading) || (visibility.price && productItemPrice) ? `
                                <div class="order-divider"></div>
                            ` : ''}

                        <form 
                            id="product-order-form" 
                            onsubmit="return event.preventDefault()" 
                            class="text-center col-sm-10 col-md-8 col-lg-6 mx-auto template-form order-form"
                            data-name="${visibility.heading && productItemHeading ? productItemHeading : ''}"
                            data-price="${visibility.price && productItemPrice ? `${currencySymbol} ${productItemPrice}` : ''}"
                            data-image-url="${visibility.image && productItemImgUrl  ? productItemImgUrl : ''}"
                            data-id="${productId || ''}"
                            data-position="${position || ''}"
                          >
                            <div class="row">
                                <div class="col-md-6 input-wrapper">
                                    <label class="body-text-1 d-none">Name*</label>
                                    <input name="visitor-name" type="text" placeholder="Name*" class="cs-input" />
                                    <div class="form-error name-err error-text text-start d-block cs-mt-4"></div>
                                </div>
                                <div class="col-md-6 cs-mobile input-wrapper">
                                    <label class="body-text-1 d-none">Email*</label>
                                    <input name="visitor-email" type="text" placeholder="Email Address*" id="pwd" class="cs-input" />
                                    <div class="form-error email-err error-text text-start d-block cs-mt-4"></div>
                                </div>
                            </div>

                            <div class="cs-mt-28 cs-mobile textarea-container input-wrapper">
                                <label class="body-text-1 d-none">Message</label>
                                <textarea name="visitor-message" rows="3" placeholder="Message" class="cs-input no-resize"></textarea>
                                <div class="form-error msg-err error-text text-start d-block"></div>
                            </div>

                            <div class="cs-mt-28 cs-mobile form-submit-btn-wrapper">
                                <button type="submit" class="submit-btn cs-btn col-12 cs-btn-md cs-btn-filled-2">
                                    <span class="spinner-border order-form-loader"></span>
                                    Send request
                                </button>
                            </div>

                            <div class="form-error server-error"></div>
                        </form>
                    </div>
                </div>
                <div class="order-form-success">
                    <span class="icon-wrapper d-inline-block">${successSVG()}</span>
                    <div class="server-success heading-4 text-center"></div>
                </div>
            </div>
        `;

    if (window.sendMedusa) {
      window.sendMedusa('site_order_form_viewed', {
        section: section,
        position: parseInt(position) + 1
      });
    }
    openModal({
      body: modalContent,
      fullscreen: 'mobile',
      title: 'Product order form',
    }, event);
  }

  function successSVG() {
    return `
      <svg xmlns="http://www.w3.org/2000/svg" width="120" height="120" viewBox="0 0 120 120" fill="none">
        <path d="M111.439 46.56C109.674 44.7978 108.271 42.7063 107.31 40.404C106.354 38.0999 105.863 35.6292 105.866 33.1344C105.866 27.8784 103.738 23.1396 100.306 19.6944C98.5415 17.9288 96.4462 16.5287 94.1399 15.5744C91.8335 14.6202 89.3616 14.1306 86.8656 14.1336C84.3711 14.1345 81.9009 13.6427 79.597 12.6863C77.293 11.73 75.2007 10.328 73.44 8.56083C71.6768 6.794 69.5816 5.39315 67.2751 4.43882C64.9686 3.4845 62.4962 2.99553 60 3.00003C57.5039 2.99553 55.0315 3.4845 52.7249 4.43882C50.4184 5.39315 48.3233 6.794 46.56 8.56083C44.7744 10.3464 42.684 11.7408 40.404 12.6888C38.124 13.6368 35.6544 14.1336 33.1344 14.1336C27.8784 14.1336 23.1396 16.2624 19.6944 19.6944C17.9288 21.4586 16.5287 23.5539 15.5744 25.8602C14.6202 28.1665 14.1306 30.6385 14.1336 33.1344C14.1336 35.6544 13.6404 38.1252 12.69 40.4052C11.7294 42.7075 10.3265 44.799 8.56083 46.5612C6.79414 48.3243 5.39337 50.4193 4.43905 52.7256C3.48473 55.0319 2.99569 57.5041 3.00003 60C3.00003 64.8636 4.84923 69.7284 8.56083 73.44C10.3265 75.2023 11.7294 77.2938 12.69 79.596C13.6464 81.9002 14.137 84.3709 14.1336 86.8656C14.1336 92.1216 16.2624 96.8604 19.6944 100.306C21.4586 102.071 23.5539 103.471 25.8602 104.426C28.1665 105.38 30.6385 105.869 33.1344 105.866C35.6544 105.866 38.1252 106.36 40.4052 107.31C42.6852 108.26 44.7744 109.654 46.5612 111.439C48.3243 113.206 50.4193 114.607 52.7256 115.561C55.0319 116.515 57.5041 117.004 60 117C64.8636 117 69.7284 115.151 73.44 111.439C75.2256 109.654 77.316 108.259 79.596 107.311C81.876 106.363 84.3456 105.866 86.8656 105.866C92.1216 105.866 96.8604 103.738 100.306 100.306C102.071 98.5415 103.471 96.4462 104.426 94.1399C105.38 91.8335 105.869 89.3616 105.866 86.8656C105.866 84.3456 106.36 81.8748 107.31 79.5948C108.271 77.2926 109.674 75.2011 111.439 73.4388C113.206 71.6757 114.607 69.5808 115.561 67.2745C116.515 64.9682 117.004 62.496 117 60C117 55.1364 115.151 50.2716 111.439 46.56ZM87.6012 46.7376L53.9196 80.4192C52.9687 81.3694 51.6795 81.9031 50.3352 81.9031