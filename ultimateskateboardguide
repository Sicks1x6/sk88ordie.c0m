    <script>
  (() => {  
    const init = () => {
      document.addEventListener('click', (event) => {
        if(event.defaultPrevented) return; // If the event was already handled, do nothing
        const connectOnWhatsappBtn = event.target.closest('.connect-on-whatsapp');

        if (!connectOnWhatsappBtn) return;

        window.open('https://wa.me/', '_blank');
      });
    }; 

    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', init);
    } else {
      init();
    } 
  })();
</script>


