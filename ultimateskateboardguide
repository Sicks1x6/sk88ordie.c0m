                                                serviceId: '2b8bfd96-b451-4fa5-a62d-f3237cd0283b',
                                                serviceItemImgUrl: `https://images.pexels.com/photos/1439951/pexels-photo-1439951.jpeg?auto=compress&amp;cs=tinysrgb&amp;h=650&amp;w=940`,
