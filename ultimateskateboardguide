<script>
  function openProductOrderForm(button, event, productData) {
    if(event.defaultPrevented) {
      return;
    }
    event.preventDefault();
    const position = button.getAttribute('data-position');
    const section = 'Product';

    const {
      productId,
      productItemImgUrl,
      productItemHeading,
      productItemPrice,
      currencySymbol,
      visibility,
    } = productData;

    // Create the modal content dynamically
    const modalContent = `
            <div class="order-form-wrapper" id="product-modal">
                <div class="order-content-default">
                    ${visibility.image && productItemImgUrl ? `
                        <div class="order-thumbnail">
                            <img src="${productItemImgUrl}" alt="Product Image" loading="lazy" />
                        </div>
                    ` : ''}

                    <div class="order-details">
                        ${visibility.heading && productItemHeading ? `
                            <div class="card-title heading-3 cs-mb-8">${productItemHeading}</div>
                        ` : ''}

                        <div class="card-text body-text-2">
                            ${visibility.price && productItemPrice ? `
                                <div class="product-price brand-color text-start">
                                    <span class="heading-5">${currencySymbol}</span>
                                    <span class="heading-5">${productItemPrice}</span>
                                </div>
                            ` : ''}
                        </div>

                        ${(visibility.heading && productItemHeading) || (visibility.price && productItemPrice) ? `
                                <div class="order-divider"></div>
                            ` : ''}

                        <form 
                            id="product-order-form" 
                            onsubmit="return event.preventDefault()" 
                            class="text-center col-sm-10 col-md-8 col-lg-6 mx-auto template-form order-form"
                            data-name="${visibility.heading && productItemHeading ? productItemHeading : ''}"
                            data-price="${visibility.price && productItemPrice ? `${currencySymbol} ${productItemPrice}` : ''}"
                            data-image-url="${visibility.image && productItemImgUrl  ? productItemImgUrl : ''}"
                            data-id="${productId || ''}"
                            data-position="${position || ''}"
                          >
                            <div class="row">
                                <div class="col-md-6 input-wrapper">
                                    <label class="body-text-1 d-none">Name*</label>
                                    <input name="visitor-name" type="text" placeholder="Name*" class="cs-input" />
                                    <div class="form-error name-err error-text text-start d-block cs-mt-4"></div>
                                </div>
                                <div class="col-md-6 cs-mobile input-wrapper">
                                    <label class="body-text-1 d-none">Email*</label>
                                    <input name="visitor-email" type="text" placeholder="Email Address*" id="pwd" class="cs-input" />
                                    <div class="form-error email-err error-text text-start d-block cs-mt-4"></div>
                                </div>
                            </div>

                            <div class="cs-mt-28 cs-mobile textarea-container input-wrapper">
                                <label class="body-text-1 d-none">Message</label>
                                <textarea name="visitor-message" rows="3" placeholder="Message" class="cs-input no-resize"></textarea>
                                <div class="form-error msg-err error-text text-start d-block"></div>
                            </div>

                            <div class="cs-mt-28 cs-mobile form-submit-btn-wrapper">
                                <button type="submit" class="submit-btn cs-btn col-12 cs-btn-md cs-btn-filled-2">
                                    <span class="spinner-border order-form-loader"></span>
                                    Send request
                                </button>
                            </div>

                            <div class="form-error server-error"></div>
                        </form>
                    </div>
                </div>
                <div class="order-form-success">
                    <span class="icon-wrapper d-inline-block">${successSVG()}</span>
                    <div class="server-success heading-4 text-center"></div>
                </div>
            </div>
        `;

    if (window.sendMedusa) {
      window.sendMedusa('site_order_form_viewed', {
        section: section,
        position: parseInt(position) + 1
      });
